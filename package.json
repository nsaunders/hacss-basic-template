    "codesandbox",
    "hacss",
    "atomic",
    "css"
  ],
  "author": "Nick Saunders",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hacss/hacss-basic-template/issues"
  },
  "homepage": "https://github.com/hacss/hacss-basic-template#readme",
  "devDependencies": {
    "hacss": "^0.8.6",
    "node": "^13.12.0",
    "serve": "^11.3.0"
  }
}
