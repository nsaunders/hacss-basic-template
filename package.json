{
  "name": "hacss-basic-site",
  "private": true,
  "version": "1.0.0",
  "description": "Minimalistic Hacss project template, suitable for prototyping or a static Web site where integration with an existing build process is not required",
  "main": "index.html",
  "scripts": {
    "build": "hacss --output ha.css *.html",
    "start": "serve"
